
<div class="column c-67 clearfix">
  <div class="box contactUs ">
    <h4><span>New City</span></h4>
    <div class="boxInfo contactForm">
      <%= form_for [:admin,@city] do |f| %>
        <% if @city.errors.any? %>
          <div id="error_explanation">
            <h2><%= pluralize(@city.errors.count, "error") %> prohibited this city from being saved:</h2>

            <ul>
              <% @city.errors.full_messages.each do |msg| %>
                <li><%= msg %></li>
              <% end %>
            </ul>
          </div>
        <% end %>

        <div class="field">
          <%= f.label :name %>
          <%= f.text_field :name %><br>
        </div>
      <div class="field">
          <%= f.label :state_id %>
          <%= collection_select(:state, :id, State.all, :id, :name) %>
        </div>
        <div class="actions">
          <%= f.submit "Create",:class => 'submit' %>
        </div>
      <% end %>

    </div>
  </div>
</div>
